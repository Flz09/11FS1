CREATE TABLE EMPLOYEES (
  EmployeeID INTEGER PRIMARY KEY,
  Name TEXT NOT NULL,
  Age INTEGER NOT NULL,
  Department TEXT NOT NULL
);

INSERT INTO EMPLOYEES VALUES (0001, 'Clark', 28 ,'Sales');
INSERT INTO EMPLOYEES VALUES (0002, 'Dave', 34 ,'Accounting');
INSERT INTO EMPLOYEES VALUES (0003, 'Ava', 46 ,'Sales');

SELECT Name, Age FROM EMPLOYEES WHERE age > 30;
